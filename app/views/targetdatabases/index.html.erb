<p id="notice"><%= notice %></p>

<h1>Listing Targetdatabases</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @targetdatabases.each do |targetdatabase| %>
      <tr>
        <td><%= link_to 'Show', targetdatabase %></td>
        <td><%= link_to 'Edit', edit_targetdatabase_path(targetdatabase) %></td>
        <td><%= link_to 'Destroy', targetdatabase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Targetdatabase', new_targetdatabase_path %>
